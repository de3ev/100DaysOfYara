      $api2 = "RegOpenKeyEx" //Persistence
      $api3 = "RegSetValueEx" //Persistence
      $api4 = "RegDeleteKeyEx" //Persistence
      $api5 = "RegGetValue" //Persistence
  
    condition:
      3 of ($api*)
  }

